{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Docker: Attach to Node",
      "type": "node",
      "request": "attach",
      "restart": true,
      "port": 9222,
      "remoteRoot": "usr/workspace/tdd-node-api/dist",
      "protocol": "inspector",
      "resolveSourceMapLocations": [
        "usr/workspace/tdd-node-api/dist/**",
        "!node_modules",
      ]
    }
  ]
}