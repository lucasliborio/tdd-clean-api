{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Docker: Attach to Node",
      "type": "node",
      "request": "attach",
      "restart": true,
      "port": 9222,
      "remoteRoot": "/USR/WORKSPACE/TDD-NODE-API",
      "protocol": "inspector",
      "resolveSourceMapLocations": [
        "/USR/WORKSPACE/TDD-NODE-API/**",
        "!node_modules",
      ]
    }
  ]
}